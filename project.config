[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/TheDoctor12
[access "refs/for/refs/*"]
	read = group user/TheDoctor12
	push = group user/TheDoctor12
	label-Code-Review = -2..+2 group user/TheDoctor12
	label-Verified = -1..+1 group user/TheDoctor12
	rebase = group user/TheDoctor12
	removeReviewer = group user/TheDoctor12
	submit = group user/TheDoctor12
[access "refs/heads/*"]
	read = group user/TheDoctor12
	create = group user/TheDoctor12
	push = +force group user/TheDoctor12
	pushMerge = group user/TheDoctor12
[access "refs/tags/*"]
	push = +force group user/TheDoctor12
	pushTag = group user/TheDoctor12
	pushSignedTag = group user/TheDoctor12
